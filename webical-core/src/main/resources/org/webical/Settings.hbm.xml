<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.webical" default-lazy="false">
	<class name="Settings" table="settings">

		<id name="settingsId" type="java.lang.Long">
            <column name="settingsId" />
            <generator class="identity" />
  		</id>

		<set name="options" cascade="all" lazy="false" table="options">
			<key column="settingsId2" not-null="true" />
			<one-to-many class="Option" />
		</set>

	</class>
</hibernate-mapping>