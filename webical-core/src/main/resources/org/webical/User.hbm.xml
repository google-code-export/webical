<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.webical" default-lazy="false">
	<class name="User" table="wcaluser">

  		<id name="userId"/>

    	<property length="1000" name="firstName" />
		<property length="1000" name="lastNamePrefix" />
		<property length="1000" name="lastName" />
		<property type="date" name="birthDate" />

	</class>
</hibernate-mapping>